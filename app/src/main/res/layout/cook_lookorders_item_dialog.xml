<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/cook_lookorders_item_orderidtext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginTop="10dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="10dp"
        android:textSize="16sp"
        android:textColor="#000000"
        android:text="订单号" />

    <TextView
        android:id="@+id/cook_lookorders_item_orderid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginTop="10dp"
        android:layout_alignRight="@+id/cook_lookorders_item_line"
        android:layout_marginRight="10dp"
        android:gravity="right"
        android:textSize="16sp"
        android:textColor="#000000" />

    <TextView
        android:id="@+id/cook_lookorders_item_ordertimetext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/cook_lookorders_item_orderidtext"
        android:layout_marginTop="10dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="10dp"
        android:textSize="16sp"
        android:textColor="#000000"
        android:text="下单时间" />

    <TextView
        android:id="@+id/cook_lookorders_item_ordertime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/cook_lookorders_item_orderid"
        android:layout_marginTop="10dp"
        android:layout_alignRight="@+id/cook_lookorders_item_line"
        android:layout_marginRight="10dp"
        android:gravity="right"
        android:textSize="16sp"
        android:textColor="#000000" />

    <TextView
        android:id="@+id/cook_lookorders_item_orderusernametext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/cook_lookorders_item_ordertimetext"
        android:layout_marginTop="10dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="10dp"
        android:textSize="16sp"
        android:textColor="#000000"
        android:text="用户名" />

    <TextView
        android:id="@+id/cook_lookorders_item_orderusername"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/cook_lookorders_item_ordertime"
        android:layout_marginTop="10dp"
        android:layout_alignRight="@+id/cook_lookorders_item_line"
        android:layout_marginRight="10dp"
        android:gravity="right"
        android:textSize="16sp"
        android:textColor="#000000" />

    <View
        android:id="@+id/cook_lookorders_item_line"
        android:layout_width="250dp"
        android:layout_height="2dp"
        android:layout_below="@+id/cook_lookorders_item_orderusernametext"
        android:background="#000000"
        android:layout_marginTop="10dp"/>

    <ListView
        android:id="@+id/cook_lookorders_item_listview"
        android:layout_width="230dp"
        android:layout_height="240dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="10dp"
        android:layout_below="@+id/cook_lookorders_item_line"
        android:layout_marginTop="10dp"
        android:scrollbars="none"
        android:listSelector="@android:color/transparent"
        android:divider="#00000000"
        android:dividerHeight="10dp" />

    <Button
        android:id="@+id/cook_lookorders_item_submit"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/cook_lookorders_item_listview"
        android:layout_alignParentLeft="true"
        android:layout_marginTop="10dp"
        android:background="#ffffff" />

</RelativeLayout>